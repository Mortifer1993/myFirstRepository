function Accumulator (value){
  this.value = value;

  this.read = function () {
    this.a = +prompt ("input value a");
    this.value = this.value + this.a;
    return this.value;

  };
};



let accumulator = new Accumulator(1); 

accumulator.read(); 
accumulator.read(); 

alert(accumulator.value);
